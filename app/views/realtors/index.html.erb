
<% @accounts.each do |account| %>
   
    <div class="container1">
    <div class="col-md-12 mt-8">
    <div class="">
    <div class="text-center border p-4 pb-4 ml-lg-5">
     <div class="circle ml-4">
        <%= image_tag account.image1.url (:medium) %>
      </div>
      <h3 class="h4 m-0"><%= account.full_name %></h3>
      <p><%= account.company %></p>
 
      <div class="row">
        <div class="col-12 pr-1 pl-1">
           <button class="btn btn-block btn-info text-uppercase mb-3" id="show-tel" data-telephone="<%= account.telephone %>"><i class="fas fa-phone mr-2"></i><span>Call</span></button>
          </div>
        </div>
 
     <div class="row">
       <div class="col-md-12 pr-1 pl-1 pb-1">
         <%= link_to "Profile", profile_path(account.id), class: "btn btn-secondary btn-block text-uppercase" %>
         </div> 
            
         </div>
      </div>
   </div>
   </div>
   </div>
<% end %>
